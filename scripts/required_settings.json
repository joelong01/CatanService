{
    "resourceGroup": {
        "description": "the Azure Resource Group for the app",
        "shellscript": "",
        "default": "",
        "tempEnvironmentVariableName": "AZURE_RESOURCE_GROUP",
        "requiredAsEnv": true
    },
    "cosmosAccount": {
        "description": "the Cosmosdb account name",
        "shellscript": "",
        "default": "user-cosmos-account",
        "tempEnvironmentVariableName": "COSMOS_ACCOUNT_NAME",
        "requiredAsEnv": true
    },
    "hostName": {
        "description": "the DNS name for the application",
        "shellscript": "",
        "default": "localhost:8080",
        "tempEnvironmentVariableName": "",
        "requiredAsEnv": false
    },
    "azureLocation": {
        "description": "the location for your Azure Datacenter",
        "shellscript": "",
        "default": "uswest3",
        "tempEnvironmentVariableName": "",
        "requiredAsEnv": false
    },
    "keyVaultName": {
        "description": "the name of the keyvault used by the app",
        "shellscript": "",
        "default": "",
        "tempEnvironmentVariableName": "",
        "requiredAsEnv": false
    },
    "cosmosDatabaseName": {
        "description": "the name of the User database",
        "shellscript": "",
        "default": "Users-db",
        "tempEnvironmentVariableName": "",
        "requiredAsEnv": false
    },
    "adminEmail": {
        "description": "the email of the admin",
        "shellscript": "",
        "default": "",
        "tempEnvironmentVariableName": "",
        "requiredAsEnv": false
    },
    "adminProfileJson": {
        "description": "the location of a JSON doc that has the admin profile",
        "shellscript": "",
        "default": "",
        "tempEnvironmentVariableName": "",
        "requiredAsEnv": false
    },
    "testCredCacheLocation": {
        "description": "the location to store the cached creds when running tests",
        "shellscript": "",
        "default": "$HOME/.catanServiceCredCache",
        "tempEnvironmentVariableName": "",
        "requiredAsEnv": false
    },
    "adminPassword": {
        "description": "the password for the admin account",
        "shellscript": "",
        "default": "",
        "tempEnvironmentVariableName": "",
        "requiredAsEnv": false
    },
    "testUsersJson": {
        "description": "the location of a JSON doc that has test users",
        "shellscript": "",
        "default": "./test_users.json",
        "tempEnvironmentVariableName": "",
        "requiredAsEnv": false
    },
    "rustLog": {
        "description": "the settings for rust logger",
        "shellscript": "",
        "default": "actix_web=trace,actix_server=trace,rust=trace",
        "tempEnvironmentVariableName": "",
        "requiredAsEnv": false
    },
    "testPhoneNumber": {
        "description": "the phone number to use in tests (*to*)",
        "shellscript": "",
        "default": "",
        "tempEnvironmentVariableName": "",
        "requiredAsEnv": false
