        "description": "the azure communication connection string",
        "shellscript": "./get_azure_info.sh --comm-string",
        "default": "",
        "tempEnvironmentVariableName": "AZURE_COMMUNICATION_CONNECTION_STRING",
        "requiredAsEnv": true
    },
    "keyCloakClientSecret": {
        "description": "the secret the service needs to be a key clock 'client'",
        "shellscript": "",
        "default": "",
        "tempEnvironmentVariableName": "",
        "requiredAsEnv": false
    },
