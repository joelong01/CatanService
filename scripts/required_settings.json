        "requiredAsEnv": false
    },
    "validationSecretKey": {
        "description": "the secret key used for email validation",
        "shellscript": "./get_azure_info.sh --validation-secret-key",
        "default": "",
        "tempEnvironmentVariableName": "VALIDATION_SECRET_KEY",
        "requiredAsEnv": false
    },
