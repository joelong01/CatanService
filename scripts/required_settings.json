    "keyCloakAdminPassword": {
        "description": "the password for the KeyCloak admin account",
        "shellscript": "",
        "default": "",
        "tempEnvironmentVariableName": "KEYCLOAK_ADMIN_PASSWORD",
        "requiredAsEnv": true
    },
    "keyCloakTestPassword": {
        "description": "the password for the KeyCloak test client(s)",
        "shellscript": "",
        "default": "",
        "tempEnvironmentVariableName": "",
        "requiredAsEnv": false
    },
    "keyCloakResourceGroup": {
        "description": "the Resource Group to install Key Cloak into",
        "shellscript": "",
        "default": "",
        "tempEnvironmentVariableName": "KEYCLOAK_RESOURCE_GROUP",
        "requiredAsEnv": true
    }
}